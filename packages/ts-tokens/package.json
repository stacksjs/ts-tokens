{
  "name": "ts-tokens",
  "type": "module",
  "version": "0.1.0",
  "description": "A TypeScript library for managing fungible and non-fungible tokens on Solana.",
  "author": "Chris Breuer <chris@stacksjs.org>",
  "license": "MIT",
  "homepage": "https://github.com/stacksjs/ts-tokens#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stacksjs/ts-tokens.git"
  },
  "bugs": {
    "url": "https://github.com/stacksjs/ts-tokens/issues"
  },
  "keywords": [
    "typescript",
    "solana",
    "token",
    "tokens",
    "nft",
    "spl",
    "metaplex",
    "blockchain",
    "web3",
    "stacksjs"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./token": {
      "types": "./dist/token/index.d.ts",
      "import": "./dist/token/index.js"
    },
    "./nft": {
      "types": "./dist/nft/index.d.ts",
      "import": "./dist/nft/index.js"
    },
    "./config": {
      "types": "./dist/config/config.d.ts",
      "import": "./dist/config/config.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js"
    },
    "./drivers": {
      "types": "./dist/drivers/index.d.ts",
      "import": "./dist/drivers/index.js"
    },
    "./storage": {
      "types": "./dist/storage/index.d.ts",
      "import": "./dist/storage/index.js"
    },
    "./programs": {
      "types": "./dist/programs/index.d.ts",
      "import": "./dist/programs/index.js"
    },
    "./security": {
      "types": "./dist/security/index.d.ts",
      "import": "./dist/security/index.js"
    },
    "./staking": {
      "types": "./dist/staking/index.d.ts",
      "import": "./dist/staking/index.js"
    },
    "./batch": {
      "types": "./dist/batch/index.d.ts",
      "import": "./dist/batch/index.js"
    },
    "./multisig": {
      "types": "./dist/multisig/index.d.ts",
      "import": "./dist/multisig/index.js"
    },
    "./governance": {
      "types": "./dist/governance/index.d.ts",
      "import": "./dist/governance/index.js"
    },
    "./defi": {
      "types": "./dist/defi/index.d.ts",
      "import": "./dist/defi/index.js"
    },
    "./marketplace": {
      "types": "./dist/marketplace/index.d.ts",
      "import": "./dist/marketplace/index.js"
    },
    "./indexer": {
      "types": "./dist/indexer/index.d.ts",
      "import": "./dist/indexer/index.js"
    },
    "./wallets": {
      "types": "./dist/wallets/index.d.ts",
      "import": "./dist/wallets/index.js"
    },
    "./analytics": {
      "types": "./dist/analytics/index.d.ts",
      "import": "./dist/analytics/index.js"
    },
    "./fluent": {
      "types": "./dist/fluent/index.d.ts",
      "import": "./dist/fluent/index.js"
    },
    "./simple-nft": {
      "types": "./dist/simple-nft/index.d.ts",
      "import": "./dist/simple-nft/index.js"
    },
    "./pnft": {
      "types": "./dist/pnft/index.d.ts",
      "import": "./dist/pnft/index.js"
    },
    "./events": {
      "types": "./dist/events/index.d.ts",
      "import": "./dist/events/index.js"
    },
    "./treasury": {
      "types": "./dist/treasury/index.d.ts",
      "import": "./dist/treasury/index.js"
    },
    "./voting": {
      "types": "./dist/voting/index.d.ts",
      "import": "./dist/voting/index.js"
    },
    "./i18n": {
      "types": "./dist/i18n/index.d.ts",
      "import": "./dist/i18n/index.js"
    },
    "./debug": {
      "types": "./dist/debug/index.d.ts",
      "import": "./dist/debug/index.js"
    },
    "./*": {
      "import": "./dist/*"
    }
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "tokens": "./dist/cli.js"
  },
  "files": [
    "README.md",
    "dist"
  ],
  "scripts": {
    "build": "bun --bun build.ts && bun run compile",
    "compile": "bun build ./bin/cli.ts --compile --minify --outfile ./bin/tokens",
    "compile:all": "bun run compile:linux-x64 && bun run compile:linux-arm64 && bun run compile:windows-x64 && bun run compile:darwin-x64 && bun run compile:darwin-arm64",
    "compile:linux-x64": "bun build ./bin/cli.ts --compile --minify --target=bun-linux-x64 --outfile ./bin/tokens-linux-x64",
    "compile:linux-arm64": "bun build ./bin/cli.ts --compile --minify --target=bun-linux-arm64 --outfile ./bin/tokens-linux-arm64",
    "compile:windows-x64": "bun build ./bin/cli.ts --compile --minify --target=bun-windows-x64 --outfile ./bin/tokens-windows-x64.exe",
    "compile:darwin-x64": "bun build ./bin/cli.ts --compile --minify --target=bun-darwin-x64 --outfile ./bin/tokens-darwin-x64",
    "compile:darwin-arm64": "bun build ./bin/cli.ts --compile --minify --target=bun-darwin-arm64 --outfile ./bin/tokens-darwin-arm64",
    "lint": "bunx --bun eslint --flag unstable_ts_config .",
    "lint:fix": "bunx --bun eslint --flag unstable_ts_config . --fix",
    "prepublishOnly": "bun --bun run build && bun run compile:all",
    "release": "bun publish --access public --provenance",
    "test": "bun test",
    "test:binary": "bash scripts/test-binary.sh",
    "typecheck": "bun --bun tsc --noEmit"
  },
  "dependencies": {
    "@solana/web3.js": "^1.95.0",
    "@solana/spl-token": "^0.4.14",
    "chalk": "^5.6.2",
    "ora": "^8.0.0",
    "@inquirer/prompts": "^7.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "better-dx": "^0.2.5"
  },
  "peerDependencies": {
    "bunfig": "^0.5.0"
  },
  "lint-staged": {
    "*.{js,ts}": "bunx --bun eslint --flag unstable_ts_config --fix"
  }
}
